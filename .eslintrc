{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "window": true,
    "console": true,
    "_": true,
    "UAParser": true,
    "uuid": true,
    "debug": true,
    "moment": true
  },
  "rules": {
    "no-console": 0,
    "angular/log": 0,
    "angular/window-service": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  }
}
